<map
  id="map"
  longitude="{{longitude}}"
  latitude="{{latitude}}"
  scale="{{scale}}"
  show-location="{{true}}"
  bindmarkertap="onMarkerTap"
  bindregionchange="onRegionChange"
  style="width: 100%; height: 100%;"
>
  <block wx:for="{{markers}}" wx:key="id">
    <cover-view class="marker-container" style="left: {{item.x}}px; top: {{item.y}}px;">
      <cover-image class="marker-image" src="{{item.iconPath}}"></cover-image>
 